{
    "name": "@donmahallem/turbo-pb",
    "version": "0.3.19",
    "description": "Protobuf Helpers for express.js",
    "main": "./dist/cjs/index.cjs",
    "types": "./dist/types/index.d.ts",
    "module": "./dist/esm/index.mjs",
    "private": false,
    "keywords": [
        "express",
        "helpers"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.js",
        "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
        "test": "mocha --config ../../.mocharc.json",
        "test:coverage": "c8 --config ../../.nycrc.json npm run test",
        "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
        "docs": "typedoc --options  ./typedoc.config.js",
        "prepublishOnly": "npm run test && npm run docs && npm run build",
        "version": "npm run build:readme && git add README.md"
    },
    "typedocMain": "./src/index.ts",
    "homepage": "https://donmahallem.github.io/js-libs/",
    "author": {
        "name": "donmahallem",
        "email": "donmahallem@users.noreply.github.com",
        "url": "https://github.com/donmahallem"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/donmahallem/js-libs/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/donmahallem/js-libs.git"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "dist/*",
        "src/*",
        "!src/**/*.spec.ts"
    ],
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/supertest": "2.0.12",
        "c8": "7.11.0",
        "chai": "4.3.6",
        "express": "4.17.3",
        "mocha": "9.2.2",
        "protobufjs": "6.11.2",
        "supertest": "6.2.2"
    },
    "peerDependencies": {
        "axios": "^0.26.0",
        "express": "^4.17.1",
        "protobufjs": "^6.10.2"
    },
    "dependencies": {
        "@donmahallem/turbo": "^2.3.16"
    }
}
